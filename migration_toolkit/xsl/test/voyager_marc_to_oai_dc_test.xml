    <!-- Item Title -->
    <utfx:test>
        <utfx:name>Item title</utfx:name>
        <utfx:assert-equal>
            <utfx:source validate="no">
                <collection xmlns="http://www.loc.gov/MARC21/slim">
                    <record>
                        <leader>00000nam a22000002a 4500</leader>
                      <controlfield tag="008">050419s1891    gw     fs           eng d</controlfield>
                        <datafield tag="245" ind1="1" ind2="0">
                            <subfield code="a">Ein koptisches Fragment einer Moses-Adam-Apokalypse</subfield>
                            <subfield code="b">shared perspectives in Australian heritage interpretation /</subfield>
                            <subfield code="h">[electronic resource] /</subfield>
                            <subfield code="c">von Adolf Harnack und Carl Schmidt.</subfield>
                        </datafield>
                    </record>
                </collection>
            </utfx:source>
            <utfx:expected validate="no">
                
                <dc:title>Ein koptisches Fragment einer Moses-Adam-Apokalypse: shared perspectives in Australian heritage interpretation</dc:title>
                    <dc:format>electronic resource</dc:format>
                
            <dc:language>eng</dc:language>
            </utfx:expected>
        </utfx:assert-equal>
    </utfx:test>
